From 0bf98151e12936b94f8c7f498367ad3ad30e9605 Mon Sep 17 00:00:00 2001
From: Mat Booth <mat.booth@gmail.com>
Date: Thu, 23 Oct 2025 11:31:03 +0100
Subject: [PATCH 1/2] relax dependency requirements

---
 Cargo.toml             | 4 ++--
 crates/make/Cargo.toml | 3 +--
 2 files changed, 3 insertions(+), 4 deletions(-)

diff --git a/Cargo.toml b/Cargo.toml
index 9a08751..a0b9cea 100644
--- a/Cargo.toml
+++ b/Cargo.toml
@@ -36,7 +36,7 @@ vector = { path = "./crates/vector" }
 bumpalo.workspace = true
 dary_heap.workspace = true
 paste.workspace = true
-pgrx = { version = "=0.16.0", default-features = false, features = ["cshim"] }
+pgrx = { version = "0.16.0", default-features = false, features = ["cshim"] }
 pgrx-catalog = "0.3.1"
 rand.workspace = true
 seq-macro.workspace = true
@@ -46,7 +46,7 @@ validator.workspace = true
 zerocopy.workspace = true
 
 [target.'cfg(all(any(target_arch = "x86_64", target_arch = "aarch64"), any(target_os = "linux", target_os = "macos")))'.dependencies]
-mimalloc = { version = "0.1.48", features = ["local_dynamic_tls"] }
+mimalloc = { version = ">=0.1.46, <0.1.49", features = ["local_dynamic_tls"] }
 
 [lints]
 workspace = true
diff --git a/crates/make/Cargo.toml b/crates/make/Cargo.toml
index 66a7f52..eb7fe8e 100644
--- a/crates/make/Cargo.toml
+++ b/crates/make/Cargo.toml
@@ -5,10 +5,9 @@ edition.workspace = true
 publish = false
 
 [dependencies]
-clap = { version = "4.5.46", features = ["derive", "env"] }
+clap = { version = ">=4.5.45, <4.5.49", features = ["derive", "env"] }
 object = { version = "0.37.3", default-features = false, features = [
     "read",
-    "wasm",
 ] }
 shlex = "1.3.0"
 target-triple = "0.1.4"
-- 
2.51.0

